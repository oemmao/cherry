create table comment_test (
comment_id int(10) unsigned not null auto_increment,
board_num int(10) unsigned not null,
name varchar(30) not null,
passwd varchar(30) not null,
comment text not null,
commentDate datetime not null,
modifyDate datetime not null,
deleteDate datetime not null,
deleteCheck varchar(1) not null default 'F',
cmt_reply int(10) unsigned not null default '0',
primary key(comment_id),
foreign key(board_num) references boardtest(num)
);
create table comment_test ( comment_id int(10) unsigned not null auto_increment, board_num int(10) unsigned not null, name varchar(30) not null, passwd varchar(30) not null, comment text not null, commentDate datetime not null, modifyDate datetime not null, deleteDate datetime not null, deleteCheck varchar(1) not null default 'F', cmt_reply int(10) unsigned not null default '0', primary key(comment_id), foreign key(board_num) references boardtest(num) );
